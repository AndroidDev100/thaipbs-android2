<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:angle="270"
                android:centerColor="#005A5D5A"
                android:centerY="0.75"
                android:endColor="#00747674"
                android:startColor="#009D9E9D" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:centerColor="#00FFB600"
                    android:centerY="0.75"
                    android:endColor="#00FFCB00"
                    android:startColor="#00FFD300" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:endColor="#009FFF"
                    android:startColor="#009FFF" />
            </shape>
        </clip>
    </item>

</layer-list>